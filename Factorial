import java.util.*;
public class Main
{  
 public static void main(String args[]){  
  int i,fact=1;  
  long number;
   Scanner sc=new Scanner(System.in);
   System.out.println("enter number");
   number=sc.nextLong();
  for(i=1;i<=number;i++){    
      fact=fact*i;    
  }    
  System.out.println("Factorial of "+number+" is: "+fact);    
 }  
}
